        <%= form_with model: list, url: lists_path, local: true do |f| %>
          <div class="form-group">
            <label>タイトル</label>
            <%= f.text_field :name, class: "form-control" %>
          </div>
          <div class="form-group">
            <label>住居費</label>
            <%= f.text_field :house, class: "form-control" %>
          </div>  
          <div class="form-group">
            <label>食費</label>
            <%= f.text_field :food, class: "form-control" %>
          </div>  
          <div class="form-group">
            <label>通信費</label>
            <%= f.text_field :communication, class: "form-control" %>
          </div>  
          <div class="form-group">
            <label>光熱費</label>
            <%= f.text_field :utility, class: "form-control" %>
          </div>  
          <div class="form-group">
            <label>雑費</label>
            <%= f.text_field :miscellaneous, class: "form-control" %>
          </div>  
          <div class="form-group">
            <label>美容・服飾費</label>
            <%= f.text_field :beauty_and_clothing, class: "form-control" %>
          </div>  
          <div class="form-group">
            <label>エンタメ</label>
            <%= f.text_field :entertainment, class: "form-control" %>
          </div>  
          <div class="form-group">
            <label>教育費</label>
            <%= f.text_field :enducation, class: "form-control" %>
          </div>  
          <div class="form-group">
            <label>保険代</label>
            <%= f.text_field :insurance, class: "form-control" %>
          </div> 
          <div class="form-group">
            <label>貯蓄</label>
            <%= f.text_field :storage, class: "form-control" %>
          </div> 
          <div class="form-group">
            <label>説明</label>
            <%= f.text_field :description, class: "form-control" %>
          </div> 
        <% end %>